{
    "name": "@vitkuz/vitkuz-deepseek-adapter",
    "version": "1.1.0",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "pub": "npm publish --access public",
        "build": "tsup",
        "dev": "tsup --watch",
        "prepublishOnly": "npm run format && npm run build",
        "test": "tsx test/create-chat-completion.test.ts",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
    },
    "dependencies": {
        "openai": "^4.77.0"
    },
    "devDependencies": {
        "@types/node": "^22.10.2",
        "dotenv": "^17.2.3",
        "prettier": "^3.7.4",
        "rimraf": "^6.1.2",
        "tsup": "^8.3.5",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2"
    },
    "author": "Vitali Kuzmenka",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vitkuz/vitkuz-deepseek-adapter.git"
    },
    "bugs": {
        "url": "https://github.com/vitkuz/vitkuz-deepseek-adapter/issues"
    },
    "homepage": "https://github.com/vitkuz/vitkuz-deepseek-adapter#readme"
}
